{% spaceless %}<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<link rel="shortcut icon" href="/favicon.png">
	<title>{% block pagetitle %}Slim Framework{% endblock %}</title>
	<meta name="description" content="{% block sitedesc %}Situs jual beli lelang online di Indonesia{% endblock %}">
	<link rel="stylesheet" href="css/main.css">
	<!--[if lt IE 9]><script src="js/html5shiv.js"></script><![endif]-->
	{% block head %}{% endblock %}
</head>
<body class="{% block bodyclass %}{% endblock %}">
	<header id="header">
		<div class="container">
			<h1 class="branding"><a href="{{ path_for('homepage') }}"><abbr title="{% trans "Slim Framework" %}"><img src="/img/logo.png" class="logo" /></abbr></a></h1>
			<nav>
				<ul>
					<li><a href="#">{% trans "Nav 1" %}</a></li>
					<li><a href="#">{% trans "Nav2" %}</a></li>
				</ul>
			</nav>
		</div>
	</header>
	<main>
		{% block content %}{% endblock %}
	</main>
	<footer>
		{% block footer %}
			
		{% endblock %}
		{% block copyright %}
			<div class="container">
				<div class="copyright">&copy; {{ "now"|date("Y") }} Sunaryo Hadi.</div>
			</div>
		{% endblock %}
	</footer>
	{% block footerjs %}{% endblock %}
</body>
</html>
{% endspaceless %}